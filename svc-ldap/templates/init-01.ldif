# Note: This file is generated by Ansible
dn: olcDatabase={1}mdb,cn=config
changetype: modify
replace: olcAccess
#
#  by dn="cn=admin,{{ ldap_base_dn }}" write
#  by dn="ou=admin,ou=meta,{{ ldap_base_dn }}" write
olcAccess: {0}
  to attrs=userPassword,shadowLastChange
  by self write
  by dn="cn=adminGroup,{{ ldap_base_dn }}" write
  by anonymous auth
  by * none
#
#  by dn="cn=admin,{{ ldap_base_dn }}" write
olcAccess: {1}
  to *
  by dn="cn=adminGroup,{{ ldap_base_dn }}" write
  by * read
