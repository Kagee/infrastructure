# Managed by Ansible

server {
    listen 80 default_server;
    listen [::]:80 default_server;

    index index.html;

    location /index.html {
        root /var/www/{{ p2k16_domain_name }};
        try_files $uri $uri/ =404;
    }

    location / {
        proxy_pass http://127.0.0.1:5000;
    }
}
